
<%= form_for @propertyshowing do |f| %>

    <%= f.label :seller_id %>
    <%= f.collection_select :seller_id, Seller.all, :id, :name %>
    <br>
    <%= f.label :agent_id %>
    <%= f.collection_select :agent_id, Agent.all, :id, :name %>
    <br>
    <%= f.label :address %>
    <%= f.text_field :address %>
    <br>
    <%= f.label :description %>
    <%= f.text_field :description%>
    <br>
    <%= f.label :date %>
    <%= f.text_field :date %>
    <br>
    <%= f.label :time %>
    <%= f.text_field :time %>
    <br>
    <%= f.label :size %>
    <%= f.text_field :size %>
    <br>
    <%=f.label :price%>
    <%= f.text_field :price %>
    <br>
    <%= f.submit %>
    <% end %>
